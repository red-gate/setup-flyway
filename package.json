{
  "name": "setup-flyway",
  "version": "1.0.1",
  "description": "Setup Redgate Flyway GitHub Action",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "format": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.yml --write \"**/*.{ts,yml,yaml}\"",
    "format:check": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.yml --check \"**/*.{ts,yml,yaml}\"",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "prerelease": "yarn build",
    "release": "git add -f dist/index.js dist/cleanup.js",
    "test": "vitest run"
  },
  "repository": "git+https://github.com/red-gate/setup-flyway.git",
  "keywords": [
    "actions",
    "setup",
    "flyway"
  ],
  "author": "Redgate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/red-gate/setup-flyway/issues"
  },
  "homepage": "https://github.com/red-gate/setup-flyway#readme",
  "sideEffects": false,
  "dependencies": {
    "@actions/cache": "6.0.0",
    "@actions/core": "2.0.3",
    "@actions/exec": "3.0.0",
    "@actions/glob": "0.6.1",
    "@actions/http-client": "3.0.2",
    "@actions/io": "3.0.2",
    "@actions/tool-cache": "4.0.0",
    "fast-xml-parser": "5.3.5",
    "semver": "7.7.4",
    "xmlbuilder2": "4.0.3"
  },
  "devDependencies": {
    "@eslint-community/eslint-utils": "4.9.1",
    "@eslint/js": "10.0.1",
    "@types/node": "25.2.2",
    "@types/semver": "7.7.1",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-node": "11.1.0",
    "globals": "17.3.0",
    "nock": "14.0.11",
    "prettier": "3.8.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "packageManager": "yarn@4.9.1"
}
