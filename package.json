{
  "name": "setup-flyway",
  "version": "1.0.1",
  "description": "Setup Redgate Flyway GitHub Action",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "actions",
    "setup",
    "flyway"
  ],
  "author": "Redgate",
  "license": "MIT",
  "repository": "git+https://github.com/red-gate/setup-flyway.git",
  "homepage": "https://github.com/red-gate/setup-flyway#readme",
  "bugs": {
    "url": "https://github.com/red-gate/setup-flyway/issues"
  },
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "format": "prettier --no-error-on-unmatched-pattern --write \"**/*.{ts,yml,yaml}\"",
    "format:check": "prettier --no-error-on-unmatched-pattern --check \"**/*.{ts,yml,yaml}\"",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "packageManager": "yarn@4.9.1",
  "dependencies": {
    "@actions/cache": "6.0.0",
    "@actions/core": "3.0.0",
    "@actions/exec": "3.0.0",
    "@actions/http-client": "4.0.0",
    "@actions/tool-cache": "4.0.0",
    "@protobuf-ts/runtime": "2.11.1",
    "@protobuf-ts/runtime-rpc": "2.11.1",
    "fast-xml-parser": "5.3.6",
    "semver": "7.7.4"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/node": "25.3.0",
    "@types/semver": "7.7.1",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/eslint-plugin": "1.6.9",
    "eslint": "10.0.2",
    "eslint-plugin-import": "2.32.0",
    "nock": "14.0.11",
    "prettier": "3.8.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "vite": "7.3.1",
    "vite-plugin-checker": "0.12.0",
    "vitest": "4.0.18"
  }
}
